<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<joined-subclass name="wilos.model.misc.wilosuser.ProjectDirector"
		extends="wilos.model.misc.wilosuser.WilosUser" table="projectdirector">
		<key column="projectdirector_id"></key>

		<set name="projectMonitored" lazy="true">
			<key column="projectdirector_id"></key>
			<one-to-many class="wilos.model.misc.project.Project"></one-to-many>
		</set>
	</joined-subclass>
</hibernate-mapping>