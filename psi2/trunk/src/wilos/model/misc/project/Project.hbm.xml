<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="wilos.model.misc.project.Project" table="project" lazy="false">
		<id name="name" type="string" column="name"></id>
		<property name="description" type="java.lang.String" column="description"
			length="1024" />
		<property name="creationDate" type="java.util.Date" column="creationDate"/>
		<property name="launchingDate" type="java.util.Date" column="launchingDate"/>
		
		<set name="process" table="execute" lazy="true">
			<key column="name"></key>
			<many-to-many class="wilos.model.spem2.process.Process"
				column="process_id">
			</many-to-many>
		</set>
		
		<set name="participants" table="affectedto" lazy="true">
			<key column="name"></key>
			<many-to-many class="wilos.model.misc.wilosuser.Participant"
				column="participant_id">
			</many-to-many>
		</set>
	</class>

</hibernate-mapping>