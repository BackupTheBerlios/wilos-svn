<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	
	<class name="woops2.model.participant.Participant" table="participant" 
		lazy="false">
		
		<id name="login" type="string"
			column="login">
		</id>
		
		<property name="name" type="java.lang.String" column="name"
			length="256" />
			
		<property name="firstname" type="java.lang.String" column="firstname"
			length="256" />
			
		<property name="emailAddress" type="java.lang.String" column="emailAddress"
			length="256" />
			
		<property name="password" type="java.lang.String" column="password"
			length="256" />
			
	</class>
	
</hibernate-mapping>

