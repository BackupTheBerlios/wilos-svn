<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	
	<joined-subclass
		name="wilos.model.spem2.phase.Phase"
		extends="wilos.model.spem2.activity.Activity"
		table="phase" lazy="true">
		<key column="phase_id"></key>
		
		<set name="concretePhases" lazy="true">
			<cache usage="read-write"/>
			<key column="phase_id" />
			<one-to-many class="wilos.model.misc.concretephase.ConcretePhase" ></one-to-many>
		</set>
		
	</joined-subclass>
	
</hibernate-mapping>
