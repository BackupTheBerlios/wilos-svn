<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<joined-subclass name="wilos.model.misc.concreterole.ConcreteRoleDescriptor"
		extends="wilos.model.misc.concretebreakdownelement.ConcreteBreakdownElement"
		table="concreteroledescriptor" lazy="true">
		<key column="concreteroledescriptor_id"></key>

		<set name="concreteTaskDescriptors" lazy="true">
			<cache usage="read-write"/>
			<key column="concreteroledescriptor_id" />
			<one-to-many class="wilos.model.misc.concretetask.ConcreteTaskDescriptor"></one-to-many>
		</set>

		<many-to-one name="roleDescriptor"
			class="wilos.model.spem2.role.RoleDescriptor"
			column="roledescriptor_id">
		</many-to-one>

		<many-to-one name="participant"
			class="wilos.model.misc.wilosuser.Participant"
			column="participant_id">
		</many-to-one>

	</joined-subclass>
</hibernate-mapping>
