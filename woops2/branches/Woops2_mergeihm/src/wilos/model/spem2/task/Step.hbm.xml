<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<joined-subclass name="wilos.model.spem2.task.Step"
		extends="wilos.model.spem2.element.Element" table="step" lazy="true">
		<key column="step_id"></key>

		<many-to-one name="taskDefinition" class="wilos.model.spem2.task.TaskDefinition"
			column="taskdefinition_id">
		</many-to-one>
	</joined-subclass>
</hibernate-mapping>
