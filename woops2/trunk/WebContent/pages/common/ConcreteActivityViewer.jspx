
<f:loadBundle basename="wilos.resources.messages" var="msgs" />

<ice:form>
	<ice:panelGrid styleClass="contentLayoutGridStyle" columns="1">
		<ice:panelTabSet bgcolor="#BEB4CC">
			<br />
			<ice:panelTab label="Activity Display">
				<br />
				<ice:panelGrid styleClass="contentLayoutGridStyle" columns="2">
					<ice:outputText value="id " />
					<ice:outputText
						value="#{ActivityViewerBean.activity.id}" />
					<ice:outputText value="#{msgs['activityviewer.presentationName'] }" />
					<ice:outputText
						value="#{ActivityViewerBean.activity.presentationName}" />
					<ice:outputText value="#{msgs['activityviewer.isplanned'] }" />
					<ice:outputText
						value="#{ActivityViewerBean.activity.isPlanned}" />
					<ice:outputText value="#{msgs['activityviewer.description'] }" />
					<ice:outputText
						value="#{ActivityViewerBean.activity.description}" />
				</ice:panelGrid>
			</ice:panelTab>
		</ice:panelTabSet>
	</ice:panelGrid>
</ice:form>
