<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	
	<joined-subclass
		name="woops2.model.process.Process"
		extends="woops2.model.activity.Activity"
		table="process" lazy="true">
		<key column="process_id"></key>
	</joined-subclass>
	
</hibernate-mapping>
