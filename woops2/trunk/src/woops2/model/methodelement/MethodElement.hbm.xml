<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<joined-subclass
		name="woops2.model.methodelement.MethodElement"
		extends="woops2.model.element.Element"
		table="methodelement" lazy="true">
		<key column="methodelement_id"></key>

		<property name="authors" type="java.lang.String" column="authors"
			length="256" />

		<property name="version" type="java.lang.String"
			column="version" length="256" />

		<property name="changeDescription" type="java.lang.String"
			column="changeDescription" length="256" />

		<property name="changeDate" type="java.lang.String"
			column="changeDate" length="256" />
		
	</joined-subclass>
</hibernate-mapping>
