<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	
	<joined-subclass
		name="wilos.model.spem2.process.Process"
		extends="wilos.model.spem2.activity.Activity"
		table="process" lazy="true">
		<key column="process_id"></key>
		

		<set name="projects" lazy="true">
			<key column="process_id" />
			<one-to-many class="wilos.model.misc.project.Project" ></one-to-many>
		</set>
		
	</joined-subclass>
	
	
</hibernate-mapping>
