<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	
	<joined-subclass
		name="wilos.model.misc.concretephase.ConcretePhase"
		extends="wilos.model.misc.concreteactivity.ConcreteActivity"
		table="concretephase" lazy="true">
		<key column="concretephase_id"></key>
		
		<many-to-one name="phase" class="wilos.model.spem2.phase"
			column="phase_id" lazy="false">
	   </many-to-one>
	</joined-subclass>
</hibernate-mapping>
