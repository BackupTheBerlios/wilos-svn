<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="wilos.model.misc.concreterole.ConcreteRoleDescriptor"
		table="concreteroledescriptor" lazy="true">

		<id name="id" type="string"
			column="concreteroledescriptor_id">
			<!--identifiant auto-incremente -->
			<generator class="uuid" />
		</id>
<!--
		<property name="project" type="wilos.model.misc.project.Project"
			column="project_id" />
-->
<property name="projectId" type="java.lang.String"
			column="project_id" length="256" />
		<property name="concreteName" type="java.lang.String"
			column="concretename" length="256" />

		<set name="concreteTaskDescriptors" lazy="true">
			<key column="concreteroledescriptor_id" />
			<one-to-many class="wilos.model.misc.concretetask.ConcreteTaskDescriptor"></one-to-many>
		</set>

		<many-to-one name="roleDescriptor"
			class="wilos.model.spem2.role.RoleDescriptor"
			column="roledescriptor_id">
		</many-to-one>

		<many-to-one name="participant"
			class="wilos.model.misc.wilosuser.Participant"
			column="participant_id">
		</many-to-one>

	</class>
</hibernate-mapping>
