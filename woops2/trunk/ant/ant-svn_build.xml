<?xml version="1.0"?>
<!-- Sample build file used to retrieve svnant's sources -->
<project name="svn-test" basedir="." default="svnCheckout">
	
	 <target name="svnCheckout">
	 	
	    <property name="source-root" value="${basedir}/dist"/>
	    <property name="repository.URL" value="http://svn.berlios.de/svnroot/repos/wilos/woops2"/>
	 	
	    <java classname="org.tmatesoft.svn.cli.SVN"
	       dir="${basedir}/dist" fork="true">
	      <arg value="co"/>
	      <arg value="--username"/>
	      <arg value="admin"/>
	      <arg value="--password"/>
	      <arg value="admin"/>
	      <arg value="${repository.URL}/trunk"/>
	      <classpath>
	        <pathelement location="${basedir}/lib/ganymed.jar" />
	        <pathelement location="${basedir}/lib/svnkit.jar" />
	        <pathelement location="${basedir}/lib/svnkit-cli.jar" />
	      </classpath>
	    </java>
	  </target>

</project>
