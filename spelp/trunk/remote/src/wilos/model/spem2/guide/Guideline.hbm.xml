<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<joined-subclass
		name="wilos.model.spem2.guide.Guideline"
		extends="wilos.model.spem2.element.Element"
		table="guideline" lazy="true">
		<key column="guideline_id"></key>

		<!-- Associations -->
		<many-to-one name="taskdefinition" class="wilos.model.spem2.task.TaskDefinition"
			column="taskdefinition_id" lazy="false">
		</many-to-one>
		
	</joined-subclass>
</hibernate-mapping>
