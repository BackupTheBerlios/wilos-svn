<?xml version='1.0' encoding='UTF-8'?>


<!DOCTYPE faces-config PUBLIC
        "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
        "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>

	<application>
		<locale-config>
			<default-locale>en_US</default-locale>
		</locale-config>
	</application>

	<!--   Spring Variable resolver -->
	<application>
		<variable-resolver>
			org.springframework.web.jsf.DelegatingVariableResolver
		</variable-resolver>
		<locale-config />
	</application>
	
	<!-- Beans -->
	<managed-bean>
		<managed-bean-name>LoginBean</managed-bean-name>
		<managed-bean-class>
			wilos.presentation.web.wilosuser.LoginBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>loginService</property-name>
			<value>#{LoginService}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>ParticipantBean</managed-bean-name>
		<managed-bean-class>
			wilos.presentation.web.wilosuser.ParticipantBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>participantService</property-name>
			<value>#{ParticipantService}</value>
		</managed-property>
		<managed-property>
			<property-name>loginService</property-name>
			<value>#{LoginService}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>ProcessManagerBean</managed-bean-name>
		<managed-bean-class>
			wilos.presentation.web.wilosuser.ProcessManagerBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>processManagerService</property-name>
			<value>#{ProcessManagerService}</value>
		</managed-property>
		<managed-property>
			<property-name>loginService</property-name>
			<value>#{LoginService}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>ProjectDirectorBean</managed-bean-name>
		<managed-bean-class>
			wilos.presentation.web.wilosuser.ProjectDirectorBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>projectDirectorService</property-name>
			<value>#{ProjectDirectorService}</value>
		</managed-property>
		<managed-property>
			<property-name>loginService</property-name>
			<value>#{LoginService}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>ProjectBean</managed-bean-name>
		<managed-bean-class>
			wilos.presentation.web.project.ProjectBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>projectService</property-name>
			<value>#{ProjectService}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>RoleBean</managed-bean-name>
		<managed-bean-class>wilos.presentation.web.role.RoleBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>roleService</property-name>
			<value>#{RoleService}</value>
		</managed-property>
	</managed-bean>
	
	<!-- Managed Bean for menu interaction -->
	<managed-bean>
		<managed-bean-name>menu</managed-bean-name>
		<managed-bean-class>wilos.presentation.web.template.MenuBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>treeMenu</managed-bean-name>
		<managed-bean-class>wilos.presentation.web.template.TreeMenu</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- Managed Bean for connection view management -->
	<managed-bean>
		<managed-bean-name>connect</managed-bean-name>
		<managed-bean-class>wilos.presentation.web.template.ConnectViewBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	<!-- Managed Bean for dynamic style examples -->
	<managed-bean>
		<managed-bean-name>styleBean</managed-bean-name>
		<managed-bean-class>wilos.presentation.web.util.StyleBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<description>
			Backing bean that controls which content will be displayed
			and the MenuGroupBeans that make up the left navigation of
			the application
		</description>
		<managed-bean-name>navigation</managed-bean-name>
		<managed-bean-class>
			wilos.presentation.web.util.NavigationBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>treeNavigation</managed-bean-name>
		<managed-bean-class>
			wilos.presentation.web.util.TreeNavigation
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>



	<!-- OutputProgress Render Manager-->
	<managed-bean>
		<managed-bean-name>renderManager</managed-bean-name>
		<managed-bean-class>
			com.icesoft.faces.async.render.RenderManager
		</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>ActivityBean</managed-bean-name>
		<managed-bean-class>
			wilos.presentation.web.ActivityBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>activityService</property-name>
			<value>#{ActivityService}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>XmlFileImportBean</managed-bean-name>
		<managed-bean-class>
			wilos.presentation.web.XmlFileImportBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>processService</property-name>
			<value>#{ProcessService}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>TreeViewBean</managed-bean-name>
		<managed-bean-class>
			wilos.presentation.web.TreeViewBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>processService</property-name>
			<value>#{ProcessService}</value>
		</managed-property>
	</managed-bean>

	<validator>
		<validator-id>emailValidator</validator-id>
		<validator-class>
			wilos.presentation.web.validators.EmailValidator
		</validator-class>
	</validator>
	
	<validator>
		<validator-id>equalValidator</validator-id>
		<validator-class>
			wilos.presentation.web.validators.EqualValidator
		</validator-class>
	</validator>
	
	<navigation-rule>
		<description>Theme Reload</description>
		<navigation-case>
			<from-outcome>reload</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>


	<navigation-rule>
		<description>Custom Page for PSI2</description>

		<navigation-case>
			<from-outcome>connect</from-outcome>
			<to-view-id>/connect.iface</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>createParticipant</from-outcome>
			<to-view-id>/participantSubscribe.iface</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
    		<from-outcome>admin_main</from-outcome>
    		<to-view-id>/admin_main.iface</to-view-id>
    		<redirect />
  		</navigation-case>
  		<navigation-case>
    		<from-outcome>welcome</from-outcome>
    		<to-view-id>/welcome.iface</to-view-id>
    		<redirect />
  		</navigation-case>
  		<navigation-case>
    		<from-outcome>processManager_create</from-outcome>
    		<to-view-id>/processManager_create.iface</to-view-id>
    		<redirect />
  		</navigation-case>
  		<navigation-case>
    		<from-outcome>projectDirector_create</from-outcome>
    		<to-view-id>/projectDirector_create.iface</to-view-id>
    		<redirect />
  		</navigation-case>
  		<navigation-case>
    		<from-outcome>project_create</from-outcome>
    		<to-view-id>/project_create.iface</to-view-id>
    		<redirect />
  		</navigation-case>
  		<navigation-case>
    		<from-outcome>affect_roles</from-outcome>
    		<to-view-id>/affect_roles.iface</to-view-id>
    		<redirect />
  		</navigation-case>
	</navigation-rule>

<navigation-rule>
		<description>Custom Page for Woops2</description>

		<navigation-case>
			<from-outcome>page2</from-outcome>
			<to-view-id>/page2.iface</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>page1</from-outcome>
			<to-view-id>/page1.iface</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
